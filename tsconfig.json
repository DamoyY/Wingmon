{
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "lib": [
      "DOM",
      "ESNEXT"
    ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "strictNullChecks": true,
    "target": "ESNEXT"
  },
  "exclude": [
    "node_modules",
    "**/node_modules/*"
  ]
}